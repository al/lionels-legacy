{
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/compat": "^1.1.1",
    "cspell": "^8.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-json": "^4.0.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-stylelint": "^0.1.1",
    "eslint-plugin-yml": "^1.14.0",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-organize-imports": "^4.0.0",
    "stylelint": "^16.7.0",
    "stylelint-config-standard": "^36.0.1"
  },
  "name": "lionels-legacy",
  "packageManager": "pnpm@9.3.0",
  "private": true,
  "scripts": {
    "format": "prettier --check --ignore-unknown .",
    "format:fix": "prettier --ignore-unknown --write .",
    "lint:css": "stylelint --allow-empty-input --formatter verbose src/**/*.{css,scss}",
    "lint:css:fix": "stylelint --allow-empty-input --fix src/**/*.{css,scss}",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint --fix .",
    "lint": "pnpm run lint:js .; pnpm run lint:css",
    "lint:fix": "pnpm run lint:js:fix .; pnpm run lint:css:fix",
    "prepare": "husky",
    "spelling": "cspell .",
    "spelling:fix": "./sbin/cspell-i"
  },
  "type": "module",
  "version": "0.0.0"
}
